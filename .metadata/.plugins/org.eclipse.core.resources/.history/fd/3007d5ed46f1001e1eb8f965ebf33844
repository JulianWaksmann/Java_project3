package TP1_2048;

import java.awt.EventQueue;
import java.awt.Image;

import javax.swing.JFrame;
import java.awt.Toolkit;
import java.net.URL;

public class UIJuegoGrafico {

	private JFrame frame;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					UIJuegoGrafico window = new UIJuegoGrafico();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public UIJuegoGrafico() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		URL pp = UIJuegoGrafico.class.getResource("/TP1_2048/2048.ico");
		frame = new JFrame();
		frame.setIconImage(Toolkit.getDefaultToolkit().getImage(UIJuegoGrafico.class.getResource("/TP1_2048/2048.gif")));
		Image x = frame.getIconImage();
		frame.setTitle("2048");
		frame.setBounds(100, 100, 867, 510);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

}
